#

maindir="$(pwd)"
if [ $# != 2 ] ; then
	echo Please specify 2 revisions
	echo for example
	echo ./bin/mergeFromZK 19200 19201
	exit 1
fi
r1=$1
r2=$2

cd breeze/src/archive/web/breeze
svn merge https://svn.potix.com/svn/zk1/branches/6.0/zul/src/archive/web -r $1:$2
svn status | grep 'js$' | awk '{print $3}' | xargs svn revert {}
